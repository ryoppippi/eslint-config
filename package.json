{
	"name": "@ryoppippi/eslint-config",
	"type": "module",
	"version": "0.3.7",
	"packageManager": "pnpm@10.10.0",
	"author": "ryoppippi",
	"license": "MIT",
	"funding": "https://github.com/sponsors/ryoppippi",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ryoppippi/eslint-config.git"
	},
	"exports": {
		".": "./src/index.ts",
		"./next": "./src/next.ts",
		"./tanstack-start": "./src/tanstack-start.ts"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"./next": {
				"types": "./dist/next.d.ts",
				"default": "./dist/next.mjs"
			},
			"./tanstack-start": {
				"types": "./dist/tanstack-start.d.ts",
				"default": "./dist/tanstack-start.mjs"
			},
			"./package.json": "./package.json"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "nlx @eslint/config-inspector",
		"build": "unbuild",
		"lint": "pnpm /^lint:/",
		"lint:eslint": "eslint .",
		"lint:publint": "publint",
		"lint:typecheck": "tsc --noEmit",
		"format": "nr lint:eslint --fix",
		"prepack": "nr build && clean-pkg-json",
		"release": "nr lint && nlx bumpp && pnpm publish",
		"prepare": "simple-git-hooks"
	},
	"peerDependencies": {
		"@next/eslint-plugin-next": "^15.3.1",
		"@tanstack/eslint-plugin-query": ">= 5.74.7",
		"@tanstack/eslint-plugin-router": ">= 1.115.0",
		"eslint": ">= 9.25.1",
		"eslint-plugin-tailwindcss": "^3.18.0"
	},
	"peerDependenciesMeta": {
		"@next/eslint-plugin-next": {
			"optional": true
		},
		"@tanstack/eslint-plugin-query": {
			"optional": true
		},
		"@tanstack/eslint-plugin-router": {
			"optional": true
		},
		"eslint-plugin-tailwindcss": {
			"optional": true
		}
	},
	"dependencies": {
		"@antfu/eslint-config": "^4.11.0",
		"eslint-flat-config-utils": "^2.0.1",
		"eslint-plugin-ryoppippi": "^0.2.6",
		"pkg-types": "^2.1.0"
	},
	"devDependencies": {
		"@antfu/ni": "^24.3.0",
		"@eslint-react/eslint-plugin": "^1.48.5",
		"@next/eslint-plugin-next": "^15.3.1",
		"@tanstack/eslint-plugin-query": "^5.74.7",
		"@tanstack/eslint-plugin-router": "^1.115.0",
		"@types/eslint-plugin-tailwindcss": "^3.17.0",
		"@types/node": "^22.15.2",
		"clean-pkg-json": "^1.3.0",
		"defu": "^6.1.4",
		"eslint": "^9.25.1",
		"eslint-plugin-format": "^1.0.1",
		"eslint-plugin-react-hooks": "^5.2.0",
		"eslint-plugin-react-refresh": "^0.4.20",
		"eslint-plugin-svelte": "^3.5.1",
		"importx": "^0.5.2",
		"lint-staged": "^15.5.1",
		"pathe": "^2.0.3",
		"publint": "^0.3.12",
		"simple-git-hooks": "^2.13.0",
		"svelte": "^5.28.2",
		"typescript": "^5.8.3",
		"unbuild": "^3.5.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild",
			"simple-git-hooks"
		]
	},
	"simple-git-hooks": {
		"pre-commit": "pnpm lint-staged"
	},
	"lint-staged": {
		"*": [
			"eslint --fix"
		]
	}
}
