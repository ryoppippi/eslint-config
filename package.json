{
	"name": "@ryoppippi/eslint-config",
	"type": "module",
	"version": "0.2.1",
	"packageManager": "pnpm@10.6.5",
	"author": "ryoppippi",
	"license": "MIT",
	"funding": "https://github.com/sponsors/ryoppippi",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ryoppippi/eslint-config.git"
	},
	"exports": {
		".": "./src/index.ts",
		"./next": "./src/next.ts"
	},
	"publishConfig": {
		"access": "public",
		"exports": {
			".": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.mjs"
			},
			"./next": {
				"types": "./dist/next.d.ts",
				"default": "./dist/next.mjs"
			},
			"./package.json": "./package.json"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"dev": "nlx @eslint/config-inspector",
		"build": "unbuild",
		"lint": "pnpm /^lint:/",
		"lint:eslint": "eslint .",
		"lint:publint": "publint",
		"format": "nr lint:eslint --fix",
		"prepack": "nr build && clean-pkg-json",
		"release": "nr lint && nlx bumpp && pnpm publish",
		"prepare": "husky"
	},
	"peerDependencies": {
		"@next/eslint-plugin-next": "^15.2.4",
		"@tanstack/eslint-plugin-query": ">= 5.68.0",
		"@tanstack/eslint-plugin-router": ">= 1",
		"eslint": ">= 9.23.0",
		"eslint-plugin-tailwindcss": "^3.18.0"
	},
	"peerDependenciesMeta": {
		"@next/eslint-plugin-next": {
			"optional": true
		},
		"@tanstack/eslint-plugin-query": {
			"optional": true
		},
		"@tanstack/eslint-plugin-router": {
			"optional": true
		},
		"eslint-plugin-tailwindcss": {
			"optional": true
		}
	},
	"dependencies": {
		"@antfu/eslint-config": "^4.11.0",
		"@antfu/ni": "^24.3.0",
		"defu": "^6.1.4",
		"eslint-flat-config-utils": "^2.0.1",
		"pkg-types": "^2.1.0"
	},
	"devDependencies": {
		"@next/eslint-plugin-next": "^15.2.4",
		"@tanstack/eslint-plugin-query": "^5.68.0",
		"@tanstack/eslint-plugin-router": "^1.114.29",
		"@types/eslint-plugin-tailwindcss": "^3.17.0",
		"clean-pkg-json": "^1.2.1",
		"eslint": "^9.23.0",
		"eslint-plugin-format": "^1.0.1",
		"husky": "^9.1.7",
		"importx": "^0.5.2",
		"lint-staged": "^15.5.0",
		"publint": "^0.3.9",
		"typescript": "^5.8.2",
		"unbuild": "^3.5.0"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"lint-staged": {
		"*": [
			"eslint --fix"
		]
	}
}
